{
  "name": "vue-dynatable",
  "version": "0.1.0",
  "description": "This package allow you to manage big data in table. You could set the settings what table would do: dragging cols, resize cols, pin cols, filter and sort data. All the cells could be replaced by named slot.",
  "main": "build/index.js",
  "keywords": [
    "bigData",
    "dynamic table",
    "table on vue",
    "table"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "babel ./src -d ./lib",
    "test:unit": "vue-cli-service test:unit"
  },
  "author": {
    "name": "Safin Ilia",
    "email": "ilyshk999@gmail.com",
    "url": "https://github.com/Iacox"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
